{
    "name": "eduardocruz/laravel-telescope-mcp-server-mvp",
    "version": "0.0.3",
    "description": "A PHP MCP server for Laravel Telescope integration with Cursor IDE",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "php-mcp/server": "^1.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
    },
    "bin": [
        "bin/laravel-telescope-mcp-server"
    ],
    "autoload": {
        "psr-4": {
            "TelescopeMcp\\": "src/"
        }
    },
    "scripts": {
        "serve": "php server.php",
        "test-db": "php test-db.php",
        "mcp:inspector": "npx @modelcontextprotocol/inspector php server.php"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 